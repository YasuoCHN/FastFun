package com.klw.fastfun.pay.common.tool;

import java.util.concurrent.atomic.AtomicInteger;

/**
 * @author klwplayer.com
 *
 *         2015年3月31日
 */
public class CPParam {
	private AtomicInteger ati;
	private String vc;
	private int synRadio;

	public CPParam(String vc) {
		this.ati = new AtomicInteger(0);
		this.vc = vc;
	}

	public int getSynRadio() {
		return synRadio;
	}

	public void setSynRadio(int synRadio) {
		this.synRadio = synRadio;
	}

	public AtomicInteger getAti() {
		return ati;
	}

	public void setAti(AtomicInteger ati) {
		this.ati = ati;
	}

	public String getVc() {
		return vc;
	}

	public void setVc(String vc) {
		this.vc = vc;
	}
}
